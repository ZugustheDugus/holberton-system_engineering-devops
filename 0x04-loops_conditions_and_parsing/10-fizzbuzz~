#!/usr/bin/env bash
##This code will print numbers from 1 to 100, but will print fizz on multiples of 3, buzz on multiples of 5 and fizzbuzz on multiples of 3 and 5
i=1
while [ $i -le 100 ]
do
    if ((i % 3 == 0))
    then
	echo "fizz"
    elif ((i % 5 == 0))
	 then
	 echo "buzz"
    elif ((i % 3 == 0)) && ((i % 5 == 0))
	 then
	 echo "fizzbuzz"
    else
	echo $i
    fi
	 done
	 
